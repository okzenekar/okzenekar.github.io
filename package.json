{
  "name": "vue-test",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev",
    "predev": "rm -rf dist/*",
    "dev": "parallelshell 'npm run html' 'npm run webpack:watch' 'npm run sass' 'npm run browser-sync'",
    "webpack": "node_modules/.bin/webpack --config webpack.config.js",
    "webpack:watch": "node_modules/.bin/webpack -w --config webpack.config.js",
    "browser-sync": "./node_modules/browser-sync/bin/browser-sync.js start --port 3000 --server 'dist' --files 'dist, !dist/css/*.map'",
    "presass": "./node_modules/node-sass/bin/node-sass -r ./src/sass --o ./dist/css --include-path ./src/sass --source-map ./dist/css",
    "sass": "./node_modules/node-sass/bin/node-sass -w -r ./src/sass --o ./dist/css --include-path ./src/sass --source-map ./dist/css",
    "html": "cp src/template/index.html dist/index.html"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "browser-sync": "^2.18.6",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "node-sass": "^4.3.0",
    "parallelshell": "^2.0.0",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^2.2.0"
  },
  "dependencies": {
    "vue": "^2.1.0"
  }
}
